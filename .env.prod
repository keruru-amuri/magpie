# Environment
ENVIRONMENT=production

# API Settings
API_V1_STR=/api/v1
PROJECT_NAME=MAGPIE
DESCRIPTION=MAG Platform for Intelligent Execution - A multiagent LLM platform for aircraft MRO
VERSION=0.1.0
BACKEND_CORS_ORIGINS=["https://magpie.example.com", "https://api.magpie.example.com"]
DEBUG=false
LOG_LEVEL=info

# Azure OpenAI
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_API_VERSION=2023-12-01-preview

# Model Deployment Names
GPT_4_1_DEPLOYMENT_NAME=gpt-4-1
GPT_4_1_MINI_DEPLOYMENT_NAME=gpt-4-1-mini
GPT_4_1_NANO_DEPLOYMENT_NAME=gpt-4-1-nano

# Database
DATABASE_URL=postgresql://postgres:postgres@db:5432/magpie

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# Task Master (for compatibility with existing Task Master system)
ANTHROPIC_API_KEY=
MODEL=claude-3-7-sonnet-20250219
MAX_TOKENS=64000
TEMPERATURE=0.2
